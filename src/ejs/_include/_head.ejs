<head prefix="<%= page['prefix'] %>">
    <!-- metatags -->
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes" />
    <meta name="keywords" content="<%= page['keywords'] %>" />
    <meta name="description" content="<%= page['description'] %>" />
    <meta name="format-detection" content="telephone=no" />

    <!-- crawlersNoindexNofollow (公開時削除) -->
    <% if(process.env.NODE_ENV === 'development') { %>
    <meta name="robots" content="noindex, nofollow" />
    <% } %>

    <!-- title -->
    <title><%= page['title'] %> | <%= common['siteName'] %></title>

    <!-- CSP (サーバーで実装できない場合の代替策) -->
    <!-- <meta http-equiv="Content-Security-Policy" content="default-src 'self'; script-src 'self' maps.googleapis.com; img-src 'self' data: maps.gstatic.com *.googleapis.com *.ggpht.com *.ytimg.com *.youtube.com; child-src https://www.google.com/ https://www.youtube.com; font-src 'self' fonts.gstatic.com; style-src 'self' 'unsafe-inline' fonts.googleapis.com; base-uri 'none'; form-action 'self'; object-src 'none'; connect-src maps.googleapis.com;" /> -->

    <!-- OGP -->
    <meta property="og:locale" content="<%= common['og:locale'] %>" />
    <meta property="og:locale:alternate" content="<%= common['og:locale:alternate'] %>" />
    <meta property="og:title" content="<%= page['title'] %> | <%= common['siteName'] %>" />
    <meta property="og:type" content="<%= page['og:type'] %>" />
    <meta property="og:url" content="<%= page['og:url'] %>" />
    <meta property="og:image" content="<%= path['img:ogp'] %><%= page['og:image'] %>" />
    <meta property="og:image:secure_url" content="<%= path['img:ogp'] %><%= page['og:image:secure_url'] %>" />
    <meta property="og:image:type" content="<%= page['og:image:type'] %>" />
    <meta property="og:image:width" content="<%= page['og:image:width'] %>" />
    <meta property="og:image:height" content="<%= page['og:image:height'] %>" />
    <meta property="og:image:alt" content="<%= page['og:image:alt'] %>" />
    <meta property="og:description" content="<%= page['og:description'] %>" />
    <meta property="og:site_name" content="<%= common['og:site_name'] %>" />

    <!-- twitter card -->
    <meta name="twitter:card" content="<%= common['twitter:card']  %>" />
    <meta name="twitter:site" content="<%= common['twitter:site']  %>" />
    <meta name="twitter:image" content="<%= path['img:ogp'] %><%= page['twitter:image']  %>" />
    <meta name="twitter:image:alt" content="<%= page['twitter:image:alt']  %>" />
    <meta name="twitter:creator" content="<%= common['twitter:creator'] %>" />

    <!-- facebook -->
    <meta property="fb:app_id" content="<%= common['fb:app_id'] %>" />

    <!-- favicon RealFaviconGenerator -->
    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
    <link rel="manifest" href="/site.webmanifest" />
    <link rel="mask-icon" href="/safari-pinned-tab.svg" color="#5bbad5" />
    <meta name="apple-mobile-web-app-title" content="石井総合内科" />
    <meta name="application-name" content="石井総合内科" />
    <meta name="msapplication-TileColor" content="#da532c" />
    <meta name="theme-color" content="#ffffff" />

    <!-- if you do not use RealFaviconGenerator -->
    <!-- <link rel="shortcut icon" href="<%= path['root'] %>favicon.ico" /> -->
    <!-- <link rel="apple-touch-icon" href="<%= path['root'] %>webclip-150x150.png" /> -->

    <!-- canonical -->
    <% if(process.env.NODE_ENV === 'production') { %>
    <link rel="canonical" href="https://shogo-log1.coding11ty.com" />
    <% } %>

    <!-- google fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com" />
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin />
    <link href="https://fonts.googleapis.com/css2?family=Noto+Sans+JP:wght@400;700&display=swap" rel="stylesheet" />

    <!-- stylesheet -->
    <link rel="stylesheet" href="<%= path['css'] %>destyle.css" />
    <link rel="stylesheet" href="<%= path['css'] %>swiper-bundle.css" />
    <link rel="stylesheet" href="<%= path['css'] %>style.css" />

    <!-- script -->
    <script src="<%= path['js'] %>script.js" defer></script>

    <!-- structured data -->
    <script type="application/ld+json">
        {
            "@context": "https://schema.org/",
            "@type": "Hospital",
            "url": "https://shogo-log1.coding11ty.com",
            "logo": "/favicon-original.png",
            "name": "石井総合内科クリニック",
            "contactPoint": {
                "@type": "ContactPoint",
                "telephone": "+81-3-0000-0000",
                "contactType": "Customer service",
                "areaServed": "JP",
                "availableLanguage": "Japanese"
            },
            "location": {
                "@type": "Place",
                "@id": "https://shogo-log1.coding11ty.com",
                "name": "石井総合内科クリニック",
                "address": {
                    "@type": "PostalAddress",
                    "addressCountry": "JP",
                    "postalCode": "000-0000",
                    "addressRegion": "東京都",
                    "addressLocality": "台東区",
                    "streetAddress": "東上野"
                }
            },
            "sameAs": ["https://twwiter.com", "https://facebook.com"]
        }
    </script>
</head>
